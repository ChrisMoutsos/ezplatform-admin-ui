{
    "name": "ezsystems/ezplatform-admin-ui",
    "license": "GPL-2.0-only",
    "type": "project",
    "description": "eZ Platform Admin v2",
    "autoload": {
        "psr-4": {
          "EzSystems\\EzPlatformAdminUiBundle\\": "src/bundle/",
          "EzSystems\\EzPlatformAdminUi\\": "src/lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "EzSystems\\EzPlatformAdminUi\\Tests\\": "src/lib/Tests" }
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "../ezpublish-kernel"
        },
        {
          "type": "vcs",
          "url": "../ezplatform-user"
        },
        {
          "type": "vcs",
          "url": "../ezplatform-design-engine"
        },
        {
          "type": "vcs",
          "url": "../repository-forms"
        },
        {
          "type": "vcs",
          "url": "../ezplatform-admin-ui-modules"
        },
        {
          "type": "vcs",
          "url": "../ezplatform-richtext"
        },
        {
          "type": "vcs",
          "url": "../doctrine-dbal-schema"
        },
        {
          "type": "vcs",
          "url": "../ez-support-tools"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/http-foundation": "^4.2",
        "symfony/security": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/form": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/filesystem": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/options-resolver": "^4.2",
        "symfony/asset": "^4.2",
        "symfony/yaml": "^4.2",
        "jms/translation-bundle": "^1.3.2",
        "ezsystems/ezpublish-kernel": "dev-symfony4 as 8.0.0",
        "ezsystems/ezplatform-design-engine": "dev-symfony4 as 3.0.0",
        "ezsystems/ezplatform-richtext": "dev-symfony4 as 2.0.0",
        "ezsystems/ez-support-tools": "dev-symfony4 as 2.0.0",
        "ezsystems/repository-forms": "dev-symfony4 as 3.0.0",
        "ezsystems/ezplatform-admin-ui-modules": "dev-symfony4 as 2.0.0",
        "ezsystems/ezplatform-user": "dev-symfony4 as 2.0.0",
        "white-october/pagerfanta-bundle": "^1.1",
        "knplabs/knp-menu-bundle": "^2.1",
        "mck89/peast": "^1.8",
        "willdurand/js-translation-bundle": "^2.6.6"
    },
    "require-dev": {
        "behat/behat": "^3.5.0",
        "friendsofphp/php-cs-fixer": "~2.7.1",
        "phpunit/phpunit": "^6.4",
        "matthiasnoback/symfony-dependency-injection-test": "^2.3"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "_ezplatform_branch_for_behat_tests": "master",
        "branch-alias": {
          "dev-tmp_ci_branch": "1.5.x-dev",
          "dev-master": "1.5.x-dev"
        }
    }
}
